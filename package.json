{
  "name": "gotx-randomizer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "deploy": "./deploy.sh",
    "deploy:legacy": "node deploy.js",
    "dev": "vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepare": "husky install",
    "prettier-check": "prettier . --check",
    "prettier": "prettier . --write",
    "preview": "vite preview",
    "publish": "npm run build && npm run deploy",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:related": "vitest related",
    "updb": "npx tsc --esModuleInterop true ./scripts/parseSqlDump.cts && node ./scripts/parseSqlDump.cjs && rm ./scripts/parseSqlDump.cjs",
    "loadEnv": "export $(grep -v '^#' .env | xargs)",
    "unloadEnv": "unset $(grep -v '^#' .env | sed -E 's/(.*)=.*/\\1/' | xargs)"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.1",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.10",
    "highcharts": "^11.4.0",
    "highcharts-react-official": "^3.2.1",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.22.3",
    "react-slider": "^2.0.6",
    "sql.js": "^1.9.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.9",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-slider": "^1.3.6",
    "@types/sql.js": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "basic-ftp": "^5.0.5",
    "better-sqlite3": "^9.4.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.1.6",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{js,css,md,json}": [
      "prettier --write"
    ]
  }
}
